# Admin Login Credentials
VITE_ADMIN_USERNAME=admin
VITE_ADMIN_PASSWORD=<save_password>

# Tablet Authentication
VITE_TABLET_TOKEN=<save_token>

# Server Configuration
# Replace YOUR_SERVER_IP with your machine's actual IP address or hostname
VITE_API_URL=http://YOUR_SERVER_IP:5000
VITE_SOCKET_URL=ws://YOUR_SERVER_IP:5000

# Server binding configuration - listen on all interfaces
HOST=0.0.0.0
PORT=5000

# PDF Storage Path (for server)
PDF_STORAGE_PATH=<relative_path_to_storage>
PDF_STORAGE_URL=<absolute_file_url_to_storage>
